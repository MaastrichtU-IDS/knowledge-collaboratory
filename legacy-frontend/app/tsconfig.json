{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "esnext",
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@rjsf/material-ui/*": [
        "node_modules/@rjsf/material-ui/dist/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
