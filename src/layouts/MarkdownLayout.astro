---
import './globals.css';

import Layout from "./Layout.astro";

export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<Layout title="">
	<div style="display: flex; flex-direction: column; align-items: center;">
		<div style="max-width: 1200px; text-align: left; margin: 16px;">
			<slot />
		</div>
	</div>
</Layout>